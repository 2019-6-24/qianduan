<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>mpController</servlet-name>
        <servlet-class>com.fourth.controller.mpController</servlet-class>

        <!--
        <init-param>
            <param-name>mcAction</param-name>
            <param-value>com.fourth.model.mcAction</param-value>
        </init-param>
        -->


    </servlet>

    <servlet-mapping>
        <servlet-name>mpController</servlet-name>
        <url-pattern>/mpController</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>chartsCity</servlet-name>
        <servlet-class>com.fourth.controller.chartsCity</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>chartsCity</servlet-name>
        <url-pattern>/chartsCity</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>searchPage</servlet-name>
        <servlet-class>com.fourth.controller.searchPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchPage</servlet-name>
        <url-pattern>/searchPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cityLineCharts</servlet-name>
        <servlet-class>com.fourth.controller.cityLineCharts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cityLineCharts</servlet-name>
        <url-pattern>/cityLineCharts</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cityAreaCharts</servlet-name>
        <servlet-class>com.fourth.controller.cityAreaCharts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cityAreaCharts</servlet-name>
        <url-pattern>/cityAreaCharts</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>